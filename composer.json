{
    "name": "fsoedjede/shorturl-bundle",
    "type": "symfony-bundle",
    "description": "Provides a Shorturl Service for your Symfony Project.",
    "keywords": ["Shorturl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian Steiner",
            "email": "fab.steiner@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.6",
        "symfony/framework-bundle": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/yaml": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Fabstei\\ShorturlBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabstei\\ShorturlBundle\\Tests\\": "tests"
        }
    }
}
